{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "lib": ["DOM", "ESNext"],
    "strict": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "target": "ESNext",
    "module": "ESNext",
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
  ]
}
